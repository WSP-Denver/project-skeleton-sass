{
    "name": "wspprojectskeleton",
    "version": "1.0.0",
    "description": "",
    "main": "gulpfile.js",
    "author": "Ben Rudolph, WSP <benjamin.rudolph@wsp.com>",
    "license": "UNLICENSED",
    "private": true,
    "copyright": "WSP",
    "paths": {
        "src": {
            "base": "./src/",
            "css": "./src/include/css/",
            "fonts": "./src/include/fonts/",
            "json": "./src/json/",
            "js": "./src/js/",
            "img": "./src/images/",
            "scss": "./src/scss/"
        },
        "dist": {
            "base": "./",
            "css": "./include/css/",
            "js": "./include/js/",
            "fonts": "./include/fonts/",
            "img": "./images/"
        },
        "build": {
            "base": "./build/",
            "css": "./build/include/css/",
            "fonts": "./build/include/fonts/",
            "js": "./build/iclude/js/",
            "html": "./build/",
            "img": "./build/images/"
        },
        "favicon": {
            "src": "./src/images/favicon_src.png",
            "path": "/build/images/site/"
        },
        "scss": [],
        "templates": "./src/templates/"
    },
    "urls": {
        "live": "http:///",
        "local": "http://localhost/",
        "critical": "http://localhost/"
    },
    "vars": {
        "siteCssName": "main.css",
        "siteMqCssName": "media_queries.css",
        "vendorCssName": "vendor.css",
        "scssName": "main.scss",
        "mqscssName": "media_queries.scss",
        "cssName": "main.css"
    },
    "globs": {
        "distCss": [
            "./build/include/css/*.css",
            "./src/include/css/*.css"
        ],
        "vendorCss": [
            "./node_modules/normalize.css/normalize.css"
        ],
        "img": [
            "./images/"
        ],
        "distJs": [
            "./src/js/*.js"
        ],
        "babelJs": [
            "./src/js/*.js"
        ],
        "inlineJs": [
            "./node_modules/fontfaceobserver/fontfaceobserver.js",
            "./node_modules/loadjs/dist/loadjs.min.js"
        ],
        "critical": [
            {
                "url": "http://localhost"
            }
        ],
        "siteIcon": "./images/site/favicon.*"
    },
    "devDependencies": {
        "babel-core": "^6.26.0",
        "browser-sync": "^2.23.6",
        "chalk": "^1.1.3",
        "css-mqpacker": "^6.0.2",
        "fancy-log": "^1.3.2",
        "gulp": "^3.9.1",
        "gulp-autoprefixer": "^4.1.0",
        "gulp-babel": "^7.0.1",
        "gulp-cached": "^1.1.1",
        "gulp-concat": "^2.6.1",
        "gulp-cssnano": "^2.1.2",
        "gulp-favicons": "^2.2.6",
        "gulp-filter": "^5.1.0",
        "gulp-header": "^1.8.9",
        "gulp-if": "^2.0.2",
        "gulp-imagemin": "^3.1.1",
        "gulp-livereload": "^3.8.1",
        "gulp-load-plugins": "^1.5.0",
        "gulp-newer": "^1.4.0",
        "gulp-plumber": "^1.2.0",
        "gulp-postcss": "^7.0.1",
        "gulp-print": "^2.0.1",
        "gulp-pxtorem": "^3.0.0",
        "gulp-rename": "^1.2.2",
        "gulp-replace": "0.5.4",
        "gulp-sass": "^3.1.0",
        "gulp-size": "^2.1.0",
        "gulp-sourcemaps": "^2.6.4",
        "gulp-uglify": "^1.5.4",
        "moment": "^2.20.1",
        "pa11y": "^5.0.1",
        "precss": "^3.1.2",
        "sassdoc": "^2.5.0"
    },
    "dependencies": {
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "gulp-git": "^2.7.0",
        "normalize.css": "^5.0.0"
    },
    "scripts": {
        "start": "gulp",
        "build": "gulp build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/WSP-Denver/project-skeleton"
    },
    "bugs": {
        "email": "benjamin.rudolph@wsp.com"
    }
}
